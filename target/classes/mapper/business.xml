<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bitcamp.mapper.BusinessMapper">
<insert id="businessInsert" parameterType="com.bitcamp.dto.BusinessDTO">
insert into business (
business_no,business_name,business_division,business_addr,business_startdate,business_url,no) 
values(#{business_no},#{business_name}, #{business_division}, #{business_addr},#{business_startdate},#{business_url},#{no})
</insert>

</mapper>